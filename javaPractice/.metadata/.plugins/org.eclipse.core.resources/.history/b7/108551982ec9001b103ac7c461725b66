
public class VariableDeclaration {
	public static void main(String[] args) {
		boolean onFalse = false;
		boolean onTrue = true;
		System.out.println(onFalse);
		System.out.println(onTrue);
		
		short s = 16;
		System.out.println(s);
		
		int i = -32;
		System.out.println(i);
		
		float f = 3839.34839;
		System.out.println(f);
	}
}
